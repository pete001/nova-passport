{
    "name": "petecheyne/nova-passport",
    "description": "A Laravel Nova tool to manage laravel passport.",
    "keywords": [
        "laravel",
        "nova",
        "passport"
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.1.0"
    },
    "autoload": {
        "psr-4": {
            "Petecheyne\\Passport\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Petecheyne\\Passport\\ToolServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
